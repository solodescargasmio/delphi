  inherited qrCodigo: TFDQuery
    AfterOpen = qrCodigoAfterOpen
    Connection = con.Mysql
    SQL.Strings = (
      'select t.*, '
      'concat(v.Marca,'#39' Patente: '#39',v.Matricula) as Nombre '
      'from tarea t'
      'inner join vehiculo v on v.id=t.idVehiculo'
      'where t.id= :paId')
    Top = 24
    ParamData = <
      item
        Name = 'PAID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
