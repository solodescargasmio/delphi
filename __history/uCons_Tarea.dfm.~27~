  object qrMaterial: TFDQuery
    Connection = con.Mysql
    SQL.Strings = (
      'select ms.Cantidad,m.Nombre,md.Nombre as Medida '
      'from materiales ms'
      'inner join material m on ms.idMaterial=m.id '
      'inner join medida md on md.id=m.idMedida'
      'where ms.idTarea=:paIdT')
    Left = 632
    Top = 368
    ParamData = <
      item
        Name = 'PAIDT'
        DataType = ftInteger
        ParamType = ptInput
        Value = 1
      end>
