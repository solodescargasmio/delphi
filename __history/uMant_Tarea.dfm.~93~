  inherited FDQuery1: TFDQuery
    SQL.Strings = (
      
        'select v.id, concat(v.Marca,'#39' Patente '#39',v.Matricula) as Vehiculo' +
        ','
      'concat(c.Nombre,'#39' '#39',c.Apellido) as Cliente'
      'from vehiculo v'
      'left join cliente c on c.id=v.idCliente'
      'where v.id=:paId')
    Left = 200
    Top = 104
    ParamData = <
      item
        Name = 'PAID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
